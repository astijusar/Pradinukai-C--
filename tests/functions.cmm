func addThree(x, y, z):
    return x + y + z
endfunc

func fib(n):
    a = 0
    b = 1
    c = 0

    if(n == 0):
        return a
    endif

    for 1 to n:
        c = a + b
        a = b
        b = c
    endfor

    return b
endfunc

