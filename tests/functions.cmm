func forLoopRec(x):
    if(x < 1):
        return
    endif
    out(x)
    x = forLoopRec(x = x - 1)
endfunc

func forLoop(x):
    i = x
    for 0 to x:
        out(i)
        i = i - 1
    endfor
    out("")
endfunc

func add(x, y):
    return x + y
endfunc

func addThree(x, y, z):
    return x + y + z
endfunc

func fib(n):
    a = 0
    b = 1
    c = 0

    if(n == 0):
        return a
    endif

    for 1 to n:
        c = a + b
        a = b
        b = c
    endfor

    return b
endfunc

func fibRec(n):
    if(n < 2):
        return n
    endif
    return x=fibRec(n-1) + y=fibRec(n-2)
endfunc

n = 9
out("Normal fibonacci:")
res1 = fib(n)
out(res1)
out("Recursive fibonacci:")
res2 = fibRec(n)
out(res2)